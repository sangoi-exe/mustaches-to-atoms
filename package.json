{
  "name": "handlebars-to-react",
  "version": "1.0.0",
  "description": "Converte templates Handlebars para componentes React.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node-esm --esm src/index.ts",
    "build": "tsc",
    "analyze": "ts-node src/index.ts --mode analyze",
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "Lucas Freire Sangoi",
    "email": "lucas.sangoi@avmb.com.br"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seuusuario/handlebars-to-react.git"
  },
  "keywords": [
    "handlebars",
    "react",
    "converter",
    "jsx",
    "template"
  ],
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.26.3",
    "@babel/types": "^7.26.3",
    "@types/babel__generator": "^7.6.8",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "is-self-closing": "^1.0.1",
    "prettier": "^3.4.2",
    "reserved-words": "^0.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@glimmer/reference": "^0.93.1",
    "@glimmer/syntax": "^0.93.1",
    "@glimmer/validator": "^0.93.1",
    "commander": "^9.4.0"
  }
}
